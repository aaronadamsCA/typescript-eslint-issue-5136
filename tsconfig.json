{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "include": ["**/*"],
  "compilerOptions": {
    "baseUrl": "./",
    "isolatedModules": true,
    "jsx": "preserve",
    "module": "ES2022",
    "moduleResolution": "Node",
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "ES2022"
  }
}
